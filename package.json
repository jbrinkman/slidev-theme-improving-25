{
  "name": "slidev-theme-improving-25",
  "version": "1.2.0",
  "description": "A professional Slidev theme with Improving branding, featuring configurable footer, official color palette, and light/dark mode support",
  "type": "module",
  "author": "Joe Brinkman",
  "license": "MIT",
  "homepage": "https://github.com/jbrinkman/slidev-theme-improving-25#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbrinkman/slidev-theme-improving-25.git"
  },
  "bugs": {
    "url": "https://github.com/jbrinkman/slidev-theme-improving-25/issues"
  },
  "keywords": [
    "slidev-theme",
    "slidev",
    "presentation",
    "improving",
    "theme",
    "slides"
  ],
  "engines": {
    "node": ">=22.0.0",
    "slidev": ">=0.48.0"
  },
  "files": [
    "components",
    "layouts",
    "setup",
    "styles",
    "assets",
    "package.json",
    "README.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png",
    "prepare": "husky install"
  },
  "dependencies": {
    "@slidev/types": "^52.2.4"
  },
  "devDependencies": {
    "@slidev/cli": "^52.2.4",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.0",
    "@semantic-release/npm": "^11.0.0",
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "husky": "^8.0.3"
  },
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "fonts": {
        "sans": "Poppins",
        "mono": "Fira Code"
      }
    }
  }
}
